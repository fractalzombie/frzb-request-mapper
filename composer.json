{
    "name": "frzb/request-mapper",
    "type": "library",
    "description": "The RequestMapper component allows you to serialize JSON request to Request class and validate it",
    "keywords": [
        "library",
        "symfony",
        "component",
        "dependency",
        "injection",
        "validation",
        "serialize",
        "serializer",
        "mapper",
        "request",
        "request-mapper",
        "frzb"
    ],
    "homepage": "https://github.com/fractalzombie/frzb-request-mapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Mykhailo Shtanko",
            "email": "fractalzombie@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "fp4php/functional": "^6.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/serializer-pack": "^1.3",
        "symfony/validator": "^6.0|^7.0",
        "symfony/event-dispatcher": "^6.0|^7.0",
        "frzb/dependency-injection": "^2.0",
        "frzb/php-doc-reader": "^2.0",
        "symfony/yaml": "^6.0|^7.0",
        "symfony/property-access": "^6.0|^7.0",
        "symfony/uid": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpunit/php-code-coverage": "^10.1",
        "php-coveralls/php-coveralls": "^2.7",
        "symfony/phpunit-bridge": "^6.0|^7.0",
        "friendsofphp/php-cs-fixer": "^3.42",
        "symfony/test-pack": "^1.0.10"
    },
    "autoload": {
        "psr-4": {
            "FRZB\\Component\\RequestMapper\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
